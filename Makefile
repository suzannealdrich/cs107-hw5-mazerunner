#### Simple makefile for CS107 Java hw5##JAVAC = javacJFLAGS = -g# You could add your source files here as you create them, but# javac is usually smart enough to compile them if referenced# from the main class (or by anything from there, etc)SOURCES = MazeRunner.java Location.java Grid.javaCLASSES = $(SOURCES:.java=.class) .SUFFIXES : .java .class.java.class :	$(JAVAC) $(JFLAGS) $< default : $(CLASSES) $(OUR_CLASSES)clean : 	@echo "Removing all compiled class files..."	rm -f [A-CE-Za-z]*.class	# remove all class files not starting with D, so as to not blow away Display*.classes 